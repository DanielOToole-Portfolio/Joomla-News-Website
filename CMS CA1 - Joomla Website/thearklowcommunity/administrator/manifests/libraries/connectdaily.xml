		<language tag="fr-BE">fr-BE/fr-BE.lib_connectdaily.ini</language>
		<language tag="fr-CA">fr-CA/fr-CA.lib_connectdaily.ini</language>
		<language tag="fr-CH">fr-CH/fr-CH.lib_connectdaily.ini</language>
		<language tag="fr-FR">fr-FR/fr-FR.lib_connectdaily.ini</language>
