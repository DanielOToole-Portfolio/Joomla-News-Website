		<filename>CDailySettings.php</filename>
		<filename>class-cdaily-misc.php</filename>
		<filename>if-locale-helper.php</filename>
		<filename>CDailyAddEvent.php</filename>
        <filename>CDailySearch.php</filename>
		<filename>CDailySocialNetworkHelper.php</filename>
		<filename>CDailySSO.php</filename>
		<filename>class-joomla-locale-helper.php</filename>
		<filename>index.html</filename>
		<filename>terms.html</filename>
		<folder>css</folder>
		<folder>images</folder>
		<folder>language</folder>
		<folder>scripts</folder>
